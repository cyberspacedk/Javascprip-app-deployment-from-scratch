{
	"presets": [
		["@babel/preset-env", {
		"targets":  [
			"last 2 versions",
			"not dead",
			"not < 2%",
			"not ie 11"
			],
		"useBuiltIns": "usage"
	}], 
	"@babel/preset-react"
	],
	"plugins": [
		"react-hot-loader/babel",
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-syntax-dynamic-import"
	],
	/* "env": {
		"test": ["dynamic-import-node"]
	} */
}